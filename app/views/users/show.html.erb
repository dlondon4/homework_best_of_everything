<div class="page-header">
  <h1>User #<%= @user.id %></h1>
</div>

<dl class="dl-horizontal">
  <dt>Username</dt>
  <dd><%= @user.username %></dd>
  <dt>
  <dd>
    <ul>
      <% Favorite.where({ :user_id => @user.id}).each do |favorite| %>
        <li><%= Dish.find_by({ :id => favorite.dish_id }).name %>/<%= Venue.find_by({ :id => favorite.venue_id }).name %></li>
  <% end %>
  </ul>
  </dd>
</dt>
</dl>
